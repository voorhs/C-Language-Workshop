program
{
    int a = 51, b = 6, c;
    bool w = true;
    string x = "abc", y, z = "abcd";
    c = (a + b) * 2; if (c >= 100 or x == z)
    {
        read (y);
        write (y);
        write (x + z + not z, c);
    }

    while (c > 100)
    {
        c = c - 5;
        continue;
        write (c);
        x = x + "step";
    }
    write (x);
}
